{
  "name": "next-js-boilerplate",
  "version": "3.0.0",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "export": "next export",
    "build-prod": "run-s clean build export",
    "clean": "rimraf .next out",
    "lint": "next lint --fix",
    "check-types": "tsc --noEmit --pretty && tsc --project cypress --noEmit --pretty",
    "test": "jest",
    "commit": "cz",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3005 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3005 cypress:headless",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf .next/ && rm -rf out/ && npm install && npm run dev",
    "compile:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/nextjs-dashboard.scss src/assets/css/nextjs-dashboard.css",
    "minify:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/nextjs-dashboard.scss src/assets/css/nextjs-dashboard.min.css --output-style compressed",
    "map:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/nextjs-dashboard.scss src/assets/css/nextjs-dashboard.css --source-map true",
    "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@mui/icons-material": "^5.10.14",
    "@mui/material": "^5.10.14",
    "@mui/styled-engine-sc": "^5.10.6",
    "@mui/system": "^5.15.12",
    "@mui/x-charts": "^7.1.1",
    "@mui/x-date-pickers": "^6.2.1",
    "@reduxjs/toolkit": "^1.9.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "axios": "^1.3.4",
    "bootstrap": "4.6.2",
    "chart.js": "^3.1.0",
    "classnames": "^2.3.2",
    "cookies-next": "^2.1.1",
    "dayjs": "^1.11.7",
    "formik": "^2.2.9",
    "highlight.js": "^11.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next": "^14.1.3",
    "next-redux-wrapper": "^8.0.0",
    "next-seo": "^5.8.0",
    "node-sass": "^9.0.0",
    "node-sass-package-importer": "^5.3.2",
    "prop-types": "^15.8.1",
    "quill": "^1.3.7",
    "quill-emoji": "^0.2.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.0.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "react-quill": "^2.0.0",
    "react-redux": "^8.0.5",
    "react-syntax-highlighter": "^15.5.0",
    "reactflow": "^11.11.3",
    "reactstrap": "^9.1.5",
    "redux-persist": "^6.0.0",
    "sass": "^1.56.1",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/cz-commitlint": "^17.1.2",
    "@next/bundle-analyzer": "^13.0.0",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^29.2.0",
    "@types/lodash": "^4.14.190",
    "@types/moment": "^2.13.0",
    "@types/next-redux-wrapper": "^3.0.0",
    "@types/node": "^18.11.5",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.8",
    "@types/react-redux": "^7.1.24",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "autoprefixer": "^10.4.12",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.13",
    "cypress": "^10.11.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "eslint-plugin-testing-library": "^5.9.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "lint-staged": "^13.0.3",
    "npm-run-all2": "^5.0.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "serverless": "2.72.4",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
